                                                           Background="White"
                                                           Focusable="False" Style="{StaticResource WrittingPadControlStyle}">
                                </wp:WrittingPadControl>
                            </Grid>
                            <StackPanel Grid.Column="2">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid Grid.Column="0">
                                        <Grid>
                                            <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_1.png" Width="185" Height="143" Stretch="Fill"/>
                                            <ItemsControl ItemsSource="{Binding WirtingPadItems,Converter={StaticResource ListTakeSkipConvert},ConverterParameter=0\,1}">
                                                <ItemsControl.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <UniformGrid Rows="1" Columns="1"/>
                                                    </ItemsPanelTemplate>
                                                </ItemsControl.ItemsPanel>
                                                <ItemsControl.ItemTemplate>
                                                    <DataTemplate>
                                                        <Button Content="{Binding}" Focusable="False" 
                                                        Margin="0"
                                                        Width="185" Height="143"
                                                        FontSize="100"
                                                        Foreground="#C73B3B"
                                                        FontWeight="Bold"
                                                        kb:KeyboardControl.IsWrittingPadSelectionButton="True">
                                                            <Button.Style>
                                                                <Style TargetType="{x:Type Button}" BasedOn="{x:Null}" >
                                                                    <Setter Property="Template">
                                                                        <Setter.Value>
                                                                            <ControlTemplate TargetType="{x:Type Button}">
                                                                                <Grid>
                                                                                    <Image Name="ImgBtn" Source="/ETong.Controls.WPF;component/images/keyboard/write_1.png" Stretch="Fill"/>
                                                                                    <ContentControl x:Name="Content" Content="{TemplateBinding Content}" 
                                                                                FontSize="{TemplateBinding FontSize}" Foreground="{TemplateBinding Foreground}"
                                                                                HorizontalAlignment="Center" VerticalAlignment="Center"
                                                                                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                                                                                </Grid>
                                                                                <ControlTemplate.Triggers>
                                                                                    <Trigger Property="IsPressed" Value="True">
                                                                                        <Setter Property="Source" Value="/ETong.Controls.WPF;component/images/keyboard/write_1_press.png" TargetName="ImgBtn"/>
                                                                                        <Setter Property="Foreground" Value="White" TargetName="Content"/>
                                                                                    </Trigger>
                                                                                </ControlTemplate.Triggers>
                                                                            </ControlTemplate>
                                                                        </Setter.Value>
                                                                    </Setter>
                                                                </Style>
                                                            </Button.Style>
                                                        </Button>
                                                    </DataTemplate>
                                                </ItemsControl.ItemTemplate>
                                            </ItemsControl>
                                        </Grid>
                                    </Grid>
                                    <Grid Grid.Column="2">
                                        <Grid>
                                            <StackPanel>
                                                <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_2.png" Width="104" Height="67" Stretch="Fill" Margin="0,0,0,10"/>
                                                <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_2.png" Width="104" Height="67" Stretch="Fill"/>
                                            </StackPanel>
                                            <ItemsControl ItemsSource="{Binding WirtingPadItems,Converter={StaticResource ListTakeSkipConvert},ConverterParameter=1\,2}" Margin="0,0,0,-10">
                                                <ItemsControl.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <UniformGrid Rows="2" Columns="1"/>
                                                    </ItemsPanelTemplate>
                                                </ItemsControl.ItemsPanel>
                                                <ItemsControl.ItemTemplate>
                                                    <DataTemplate>
                                                        <Button Content="{Binding}" Focusable="False" 
                                                        Margin="0,0,0,10"
                                                        Width="104" Height="67"
                                                        FontSize="22"
                                                        Foreground="Black"
                                                        kb:KeyboardControl.IsWrittingPadSelectionButton="True">
                                                            <Button.Style>
                                                                <Style TargetType="{x:Type Button}" BasedOn="{x:Null}" >
                                                                    <Setter Property="Template">
                                                                        <Setter.Value>
                                                                            <ControlTemplate TargetType="{x:Type Button}">
                                                                                <Grid>
                                                                                    <Image Name="ImgBtn" Source="/ETong.Controls.WPF;component/images/keyboard/write_2.png" Stretch="Fill"/>
                                                                                    <ContentControl x:Name="Content" Content="{TemplateBinding Content}" 
                                                                                FontSize="{TemplateBinding FontSize}" Foreground="{TemplateBinding Foreground}"
                                                                                HorizontalAlignment="Center" VerticalAlignment="Center"
                                                                                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                                                                                </Grid>
                                                                                <ControlTemplate.Triggers>
                                                                                    <Trigger Property="IsPressed" Value="True">
                                                                                        <Setter Property="Source" Value="/ETong.Controls.WPF;component/images/keyboard/write_2_press.png" TargetName="ImgBtn"/>
                                                                                        <Setter Property="Foreground" Value="White" TargetName="Content"/>
                                                                                    </Trigger>
                                                                                </ControlTemplate.Triggers>
                                                                            </ControlTemplate>
                                                                        </Setter.Value>
                                                                    </Setter>
                                                                </Style>
                                                            </Button.Style>
                                                        </Button>
                                                    </DataTemplate>
                                                </ItemsControl.ItemTemplate>
                                            </ItemsControl>                                  
                                        </Grid>
                                    </Grid>
                                </Grid>
                                <Grid Margin="-5,5,0,0">
                                    <UniformGrid Grid.Row="1" Rows="2" Columns="4">
                                        <UniformGrid.Resources>
                                            <Style TargetType="Image">
                                                <Setter Property="Width" Value="67"/>
                                                <Setter Property="Height" Value="67"/>
                                                <Setter Property="Margin" Value="5"/>
                                            </Style>
                                        </UniformGrid.Resources>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                        <Image Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                    </UniformGrid>
                                    <!--<ItemsControl x:Name="WritingPadItemControl"  ItemsSource="{Binding WirtingPadNineItems}" d:IsHidden="True">-->
                                    <ItemsControl x:Name="WritingPadItemControl"  ItemsSource="{Binding WirtingPadItems,Converter={StaticResource ListTakeSkipConvert},ConverterParameter=3\,8}">
                                        <ItemsControl.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <UniformGrid Rows="2" Columns="4"/>
                                            </ItemsPanelTemplate>
                                        </ItemsControl.ItemsPanel>
                                        <ItemsControl.ItemTemplate>
                                            <DataTemplate>
                                                <Button Content="{Binding}" Focusable="False" 
                                                        Margin="5"
                                                        Width="67" Height="67"
                                                        FontSize="22"
                                                        Foreground="Black"
                                                        kb:KeyboardControl.IsWrittingPadSelectionButton="True">
                                                    <Button.Style>
                                                        <Style TargetType="{x:Type Button}" BasedOn="{x:Null}" >
                                                            <Setter Property="Template">
                                                                <Setter.Value>
                                                                    <ControlTemplate TargetType="{x:Type Button}">
                                                                        <Grid>
                                                                            <Image Name="ImgBtn" Source="/ETong.Controls.WPF;component/images/keyboard/write_3.png" Stretch="Fill"/>
                                                                            <ContentControl x:Name="Content" Content="{TemplateBinding Content}" 
                                                                                FontSize="{TemplateBinding FontSize}" Foreground="{TemplateBinding Foreground}"
                                                                                HorizontalAlignment="Center" VerticalAlignment="Center"
                                                                                HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                                                                        </Grid>
                                                                        <ControlTemplate.Triggers>
                                                                            <Trigger Property="IsPressed" Value="True">
                                                                                <Setter Property="Source" Value="/ETong.Controls.WPF;component/images/keyboard/write_3_press.png" TargetName="ImgBtn"/>
                                                                                <Setter Property="Foreground" Value="White" TargetName="Content"/>
                                                                            </Trigger>
                                                                        </ControlTemplate.Triggers>
                                                                    </ControlTemplate>
                                                                </Setter.Value>
                                                            </Setter>
                                                        </Style>
                                                    </Button.Style>
                                                </Button>
                                            </DataTemplate>
                                        </ItemsControl.ItemTemplate>
                                    </ItemsControl>
                                </Grid>
                            </StackPanel>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

