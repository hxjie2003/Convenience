    <!--手写键盘-->
    <Style TargetType="{x:Type kb:EditorKeyboardControl}" x:Key="WriterPadKeyboardStyle">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type kb:EditorKeyboardControl}">
                    <Border BorderThickness="0"
                                Width="{TemplateBinding Width}" Height="{TemplateBinding Height}">
                        <!--手写板-->
                        <Grid Focusable="False" HorizontalAlignment="Center" VerticalAlignment="Top">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="10"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0" Margin="0,-5,0,0">
